v0x588fcbc2a040_0 .net "select", 2 0, v0x588fcbc2a330_0;  1 drivers
v0x588fcbc2a120_0 .net "y", 0 0, L_0x588fcbc2a500;  alias, 1 drivers
L_0x588fcbc2a500 .part/v v0x588fcbc2a240_0, v0x588fcbc2a330_0, 1;
    .scope S_0x588fcbc168c0;
T_0 ;
