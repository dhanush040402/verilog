    %jmp T_1.5;
T_1.5 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x640ead920e80;
T_2 ;
    %wait E_0x640ead91cef0;
    %load/vec4 v0x640ead939950_0;
    %cmpi/e 3, 0, 3;
    %flag_get/vec4 4;
