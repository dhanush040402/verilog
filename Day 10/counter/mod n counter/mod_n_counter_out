v0x63f0bd630600_0 .net "rst", 0 0, v0x63f0bd6308f0_0;  1 drivers
E_0x63f0bd61b110 .event posedge, v0x63f0bd60ad60_0;
    .scope S_0x63f0bd61cbd0;
T_0 ;
    %wait E_0x63f0bd61b110;
    %load/vec4 v0x63f0bd630600_0;
