#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61fb6f6dc7b0 .scope module, "gray_counter_tb" "gray_counter_tb" 2 1;
 .timescale 0 0;
v0x61fb6f6ef6a0_0 .var "clk", 0 0;
v0x61fb6f6ef770_0 .net "gray", 3 0, v0x61fb6f6ef4a0_0;  1 drivers
v0x61fb6f6ef840_0 .var "rst", 0 0;
S_0x61fb6f6dc940 .scope module, "dut" "gray_counter" 2 4, 3 1 0, S_0x61fb6f6dc7b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "gray";
v0x61fb6f6cb220_0 .var "bin", 3 0;
v0x61fb6f6ef3e0_0 .net "clk", 0 0, v0x61fb6f6ef6a0_0;  1 drivers
v0x61fb6f6ef4a0_0 .var "gray", 3 0;
v0x61fb6f6ef560_0 .net "rst", 0 0, v0x61fb6f6ef840_0;  1 drivers
E_0x61fb6f6dac40 .event anyedge, v0x61fb6f6cb220_0;
E_0x61fb6f6db2e0 .event posedge, v0x61fb6f6ef3e0_0;
    .scope S_0x61fb6f6dc940;
T_0 ;
    %wait E_0x61fb6f6db2e0;
    %load/vec4 v0x61fb6f6ef560_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x61fb6f6cb220_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x61fb6f6cb220_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x61fb6f6cb220_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x61fb6f6dc940;
T_1 ;
    %wait E_0x61fb6f6dac40;
    %load/vec4 v0x61fb6f6cb220_0;
    %load/vec4 v0x61fb6f6cb220_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %xor;
    %store/vec4 v0x61fb6f6ef4a0_0, 0, 4;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x61fb6f6dc7b0;
T_2 ;
    %vpi_call 2 6 "$monitor", "clock = %d | reset = %d | count = %d | time = %0t |", v0x61fb6f6ef6a0_0, v0x61fb6f6ef840_0, v0x61fb6f6ef770_0, $time {0 0 0};
    %vpi_call 2 7 "$dumpfile", "gray_counter.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61fb6f6ef6a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61fb6f6ef840_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61fb6f6ef840_0, 0, 1;
    %delay 160, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x61fb6f6dc7b0;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x61fb6f6ef6a0_0;
    %inv;
    %store/vec4 v0x61fb6f6ef6a0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
