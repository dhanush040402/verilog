#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x589009b20140 .scope module, "palindrome_tb" "palindrome_tb" 2 1;
 .timescale 0 0;
v0x589009b34420_0 .var/i "i", 31 0;
v0x589009b34500_0 .var "num", 15 0;
v0x589009b345c0_0 .net "pal", 0 0, v0x589009b34300_0;  1 drivers
S_0x589009b202d0 .scope module, "dut" "palindrome" 2 4, 3 1 0, S_0x589009b20140;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "num";
    .port_info 1 /OUTPUT 1 "pal";
v0x589009b204b0_0 .var "d1", 3 0;
v0x589009b33f50_0 .var "d2", 3 0;
v0x589009b34030_0 .var "d3", 3 0;
v0x589009b340f0_0 .var "d4", 3 0;
v0x589009b341d0_0 .net "num", 15 0, v0x589009b34500_0;  1 drivers
v0x589009b34300_0 .var "pal", 0 0;
E_0x589009b1de00/0 .event anyedge, v0x589009b341d0_0, v0x589009b204b0_0, v0x589009b340f0_0, v0x589009b33f50_0;
E_0x589009b1de00/1 .event anyedge, v0x589009b34030_0;
E_0x589009b1de00 .event/or E_0x589009b1de00/0, E_0x589009b1de00/1;
    .scope S_0x589009b202d0;
T_0 ;
    %wait E_0x589009b1de00;
    %load/vec4 v0x589009b341d0_0;
    %pad/u 32;
    %pushi/vec4 1000, 0, 32;
    %div;
    %pushi/vec4 10, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x589009b204b0_0, 0, 4;
    %load/vec4 v0x589009b341d0_0;
    %pad/u 32;
    %pushi/vec4 100, 0, 32;
    %div;
    %pushi/vec4 10, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x589009b33f50_0, 0, 4;
    %load/vec4 v0x589009b341d0_0;
    %pad/u 32;
    %pushi/vec4 10, 0, 32;
    %div;
    %pushi/vec4 10, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x589009b34030_0, 0, 4;
    %load/vec4 v0x589009b341d0_0;
    %pad/u 32;
    %pushi/vec4 10, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x589009b340f0_0, 0, 4;
    %load/vec4 v0x589009b204b0_0;
    %load/vec4 v0x589009b340f0_0;
    %cmp/e;
    %flag_get/vec4 4;
    %jmp/0 T_0.2, 4;
    %load/vec4 v0x589009b33f50_0;
    %load/vec4 v0x589009b34030_0;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_0.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x589009b34300_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x589009b34300_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x589009b20140;
T_1 ;
    %vpi_call 2 7 "$dumpfile", "palindrome.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 1000, 0, 32;
    %store/vec4 v0x589009b34420_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x589009b34420_0;
    %cmpi/s 9999, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x589009b34420_0;
    %pad/s 16;
    %store/vec4 v0x589009b34500_0, 0, 16;
    %delay 1, 0;
    %load/vec4 v0x589009b34420_0;
    %addi 1, 0, 32;
    %store/vec4 v0x589009b34420_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x589009b20140;
T_2 ;
    %vpi_call 2 15 "$monitor", "Time=%0t | num=%0d | pal=%b", $time, v0x589009b34500_0, v0x589009b345c0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "palindrome_tb.v";
    "palindrome.v";
