    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e44811cfed0_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5e44811cfb40_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e44811cfd10_0, 0, 1;
    %delay 10, 0;
