#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f0ada913050 .scope module, "factorial_tb" "factorial_tb" 2 1;
 .timescale 0 0;
v0x5f0ada9263b0_0 .net "fact", 15 0, v0x5f0ada9133c0_0;  1 drivers
v0x5f0ada9264a0_0 .var/i "i", 31 0;
v0x5f0ada926560_0 .var "num", 3 0;
S_0x5f0ada9131e0 .scope module, "dut" "factorial" 2 5, 3 1 0, S_0x5f0ada913050;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "num";
    .port_info 1 /OUTPUT 16 "fact";
v0x5f0ada9133c0_0 .var "fact", 15 0;
v0x5f0ada9261b0_0 .var/i "i", 31 0;
v0x5f0ada926290_0 .net "num", 3 0, v0x5f0ada926560_0;  1 drivers
E_0x5f0ada910dc0 .event anyedge, v0x5f0ada926290_0, v0x5f0ada9133c0_0;
    .scope S_0x5f0ada9131e0;
T_0 ;
    %wait E_0x5f0ada910dc0;
    %load/vec4 v0x5f0ada926290_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x5f0ada926290_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %or;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 16;
    %store/vec4 v0x5f0ada9133c0_0, 0, 16;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 16;
    %store/vec4 v0x5f0ada9133c0_0, 0, 16;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x5f0ada9261b0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x5f0ada9261b0_0;
    %load/vec4 v0x5f0ada926290_0;
    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x5f0ada9133c0_0;
    %pad/u 32;
    %load/vec4 v0x5f0ada9261b0_0;
    %mul;
    %pad/u 16;
    %store/vec4 v0x5f0ada9133c0_0, 0, 16;
    %load/vec4 v0x5f0ada9261b0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5f0ada9261b0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5f0ada913050;
T_1 ;
    %vpi_call 2 7 "$monitor", "Time = %0t | number = %d | factorial = %d |", $time, v0x5f0ada926560_0, v0x5f0ada9263b0_0 {0 0 0};
    %vpi_call 2 8 "$dumpfile", "factorial.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5f0ada9264a0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x5f0ada9264a0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x5f0ada9264a0_0;
    %pad/s 4;
    %store/vec4 v0x5f0ada926560_0, 0, 4;
    %delay 10, 0;
    %load/vec4 v0x5f0ada9264a0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5f0ada9264a0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "factorial_tb.v";
    "factorial.v";
