#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x631cb242a890 .scope module, "odd_even_tb" "odd_even_tb" 2 1;
 .timescale 0 0;
v0x631cb2450030_0 .net "even", 0 0, v0x631cb243d830_0;  1 drivers
v0x631cb2450100_0 .var "num", 7 0;
v0x631cb24501d0_0 .net "odd", 0 0, v0x631cb244fee0_0;  1 drivers
S_0x631cb243d600 .scope module, "dut" "odd_even" 2 5, 3 1 0, S_0x631cb242a890;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "num";
    .port_info 1 /OUTPUT 1 "odd";
    .port_info 2 /OUTPUT 1 "even";
v0x631cb243d830_0 .var "even", 0 0;
v0x631cb244fe00_0 .net "num", 7 0, v0x631cb2450100_0;  1 drivers
v0x631cb244fee0_0 .var "odd", 0 0;
E_0x631cb243c3a0 .event anyedge, v0x631cb244fe00_0;
    .scope S_0x631cb243d600;
T_0 ;
    %wait E_0x631cb243c3a0;
    %load/vec4 v0x631cb244fe00_0;
    %parti/s 1, 0, 2;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x631cb243d830_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x631cb244fee0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x631cb244fee0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x631cb243d830_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x631cb242a890;
T_1 ;
    %vpi_call 2 7 "$monitor", "Time = %t | number = %d | odd = %d | even = %d |", $time, v0x631cb2450100_0, v0x631cb24501d0_0, v0x631cb2450030_0 {0 0 0};
    %vpi_call 2 8 "$dumpfile", "odd_even.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x631cb2450100_0, 0, 8;
    %delay 1, 0;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x631cb2450100_0, 0, 8;
    %delay 1, 0;
    %pushi/vec4 88, 0, 8;
    %store/vec4 v0x631cb2450100_0, 0, 8;
    %delay 1, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x631cb2450100_0, 0, 8;
    %delay 1, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "odd_even_tb.v";
    "odd_even.v";
