#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x594984aa3000 .scope module, "ringcounter_tb" "ringcounter_tb" 2 1;
 .timescale 0 0;
v0x594984ab5830_0 .var "clk", 0 0;
v0x594984ab5900_0 .net "q", 3 0, v0x594984ab5600_0;  1 drivers
v0x594984ab59d0_0 .var "rst", 0 0;
S_0x594984aa3190 .scope module, "dut" "ringcounter" 2 4, 3 1 0, S_0x594984aa3000;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "rst";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 4 "q";
v0x594984aa33c0_0 .net "clk", 0 0, v0x594984ab5830_0;  1 drivers
v0x594984ab5600_0 .var "q", 3 0;
v0x594984ab56e0_0 .net "rst", 0 0, v0x594984ab59d0_0;  1 drivers
E_0x594984aa1eb0 .event posedge, v0x594984ab56e0_0, v0x594984aa33c0_0;
    .scope S_0x594984aa3190;
T_0 ;
    %wait E_0x594984aa1eb0;
    %load/vec4 v0x594984ab56e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x594984ab5600_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x594984ab5600_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x594984ab5600_0;
    %parti/s 1, 3, 3;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x594984ab5600_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x594984aa3000;
T_1 ;
    %vpi_call 2 6 "$monitor", "clock = %d | reset = %d | count = %b | time = %0t |", v0x594984ab5830_0, v0x594984ab59d0_0, v0x594984ab5900_0, $time {0 0 0};
    %vpi_call 2 7 "$dumpfile", "ringcounter.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x594984ab5830_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x594984ab59d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x594984ab59d0_0, 0, 1;
    %delay 160, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x594984aa3000;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x594984ab5830_0;
    %inv;
    %store/vec4 v0x594984ab5830_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "ringcounter_tb.v";
    "ringcounter.v";
