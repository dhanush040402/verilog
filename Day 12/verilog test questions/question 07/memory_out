:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x58ddb75a4830 .scope module, "mem_8x14_example" "mem_8x14_example" 2 1;
 .timescale 0 0;
v0x58ddb75caf60_0 .var/i "i", 31 0;
v0x58ddb75cb060 .array "mem", 7 0, 13 0;
S_0x58ddb75a49c0 .scope task, "fill_memory" "fill_memory" 2 6, 2 6 0, S_0x58ddb75a4830;
 .timescale 0 0;
v0x58ddb75a4ba0_0 .var "start_value", 13 0;
TD_mem_8x14_example.fill_memory ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x58ddb75caf60_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x58ddb75caf60_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x58ddb75a4ba0_0;
    %pad/u 32;
    %load/vec4 v0x58ddb75caf60_0;
    %add;
    %pad/u 14;
    %ix/getv/s 4, v0x58ddb75caf60_0;
    %store/vec4a v0x58ddb75cb060, 4, 0;
    %load/vec4 v0x58ddb75caf60_0;
    %addi 1, 0, 32;
    %store/vec4 v0x58ddb75caf60_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
S_0x58ddb75ca660 .scope function.vec4.s14, "max_memory" "max_memory" 2 24, 2 24 0, S_0x58ddb75a4830;
 .timescale 0 0;
v0x58ddb75ca860_0 .var "dummy", 0 0;
v0x58ddb75ca920_0 .var/i "k", 31 0;
; Variable max_memory is vec4 return value of scope S_0x58ddb75ca660
TD_mem_8x14_example.max_memory ;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %load/vec4a v0x58ddb75cb060, 4;
    %ret/vec4 0, 0, 14;  Assign to max_memory (store_vec4_to_lval)
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x58ddb75ca920_0, 0, 32;
T_1.2 ;
    %load/vec4 v0x58ddb75ca920_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.3, 5;
    %retload/vec4 0; Load max_memory (draw_signal_vec4)
    %ix/getv/s 4, v0x58ddb75ca920_0;
    %load/vec4a v0x58ddb75cb060, 4;
    %cmp/u;
    %jmp/0xz  T_1.4, 5;
    %ix/getv/s 4, v0x58ddb75ca920_0;
    %load/vec4a v0x58ddb75cb060, 4;
    %ret/vec4 0, 0, 14;  Assign to max_memory (store_vec4_to_lval)
T_1.4 ;
    %load/vec4 v0x58ddb75ca920_0;
    %addi 1, 0, 32;
    %store/vec4 v0x58ddb75ca920_0, 0, 32;
    %jmp T_1.2;
T_1.3 ;
    %end;
S_0x58ddb75caac0 .scope function.vec4.s18, "sum_memory" "sum_memory" 2 14, 2 14 0, S_0x58ddb75a4830;
 .timescale 0 0;
v0x58ddb75cacd0_0 .var "dummy", 0 0;
v0x58ddb75cad90_0 .var/i "j", 31 0;
; Variable sum_memory is vec4 return value of scope S_0x58ddb75caac0
TD_mem_8x14_example.sum_memory ;
    %pushi/vec4 0, 0, 18;
    %ret/vec4 0, 0, 18;  Assign to sum_memory (store_vec4_to_lval)
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x58ddb75cad90_0, 0, 32;
T_2.6 ;
    %load/vec4 v0x58ddb75cad90_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_2.7, 5;
    %retload/vec4 0; Load sum_memory (draw_signal_vec4)
    %ix/getv/s 4, v0x58ddb75cad90_0;
    %load/vec4a v0x58ddb75cb060, 4;
    %pad/u 18;
    %add;
    %ret/vec4 0, 0, 18;  Assign to sum_memory (store_vec4_to_lval)
    %load/vec4 v0x58ddb75cad90_0;
    %addi 1, 0, 32;
    %store/vec4 v0x58ddb75cad90_0, 0, 32;
    %jmp T_2.6;
T_2.7 ;
    %end;
    .scope S_0x58ddb75a4830;
T_3 ;
    %pushi/vec4 10, 0, 14;
    %store/vec4 v0x58ddb75a4ba0_0, 0, 14;
    %fork TD_mem_8x14_example.fill_memory, S_0x58ddb75a49c0;
    %join;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x58ddb75caf60_0, 0, 32;
T_3.0 ;
    %load/vec4 v0x58ddb75caf60_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_3.1, 5;
    %vpi_call 2 39 "$display", "mem[%0d] = %0d", v0x58ddb75caf60_0, &A<v0x58ddb75cb060, v0x58ddb75caf60_0 > {0 0 0};
    %load/vec4 v0x58ddb75caf60_0;
    %addi 1, 0, 32;
    %store/vec4 v0x58ddb75caf60_0, 0, 32;
    %jmp T_3.0;
T_3.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58ddb75cacd0_0, 0, 1;
    %callf/vec4 TD_mem_8x14_example.sum_memory, S_0x58ddb75caac0;
    %vpi_call 2 41 "$display", "Sum of memory = %0d", S<0,vec4,u18> {1 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58ddb75ca860_0, 0, 1;
    %callf/vec4 TD_mem_8x14_example.max_memory, S_0x58ddb75ca660;
    %vpi_call 2 42 "$display", "Maximum value = %0d", S<0,vec4,u14> {1 0 0};
    %end;
    .thread T_3;
