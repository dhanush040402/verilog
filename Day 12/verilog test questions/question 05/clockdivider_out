#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f494c1ae210 .scope module, "tb_clk_100hz" "tb_clk_100hz" 2 3;
 .timescale -9 -12;
v0x5f494c1c1260_0 .net "clk_100hz", 0 0, v0x5f494c1ae5d0_0;  1 drivers
v0x5f494c1c1330_0 .var "clk_50mhz", 0 0;
v0x5f494c1c1400_0 .var "rst", 0 0;
S_0x5f494c1ae3a0 .scope module, "dut" "clk_100hz" 2 10, 3 4 0, S_0x5f494c1ae210;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk_50mhz";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 1 "clk_100hz";
v0x5f494c1ae5d0_0 .var "clk_100hz", 0 0;
v0x5f494c1c0fa0_0 .net "clk_50mhz", 0 0, v0x5f494c1c1330_0;  1 drivers
v0x5f494c1c1060_0 .var "count", 17 0;
v0x5f494c1c1120_0 .net "rst", 0 0, v0x5f494c1c1400_0;  1 drivers
E_0x5f494c1abe90 .event posedge, v0x5f494c1c0fa0_0;
    .scope S_0x5f494c1ae3a0;
T_0 ;
    %wait E_0x5f494c1abe90;
    %load/vec4 v0x5f494c1c1120_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 18;
    %assign/vec4 v0x5f494c1c1060_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5f494c1ae5d0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5f494c1c1060_0;
    %cmpi/e 249999, 0, 18;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 18;
    %assign/vec4 v0x5f494c1c1060_0, 0;
    %load/vec4 v0x5f494c1ae5d0_0;
    %inv;
    %assign/vec4 v0x5f494c1ae5d0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5f494c1c1060_0;
    %addi 1, 0, 18;
    %assign/vec4 v0x5f494c1c1060_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5f494c1ae210;
T_1 ;
    %delay 10000, 0;
    %load/vec4 v0x5f494c1c1330_0;
    %inv;
    %store/vec4 v0x5f494c1c1330_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5f494c1ae210;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f494c1c1330_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f494c1c1400_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f494c1c1400_0, 0, 1;
    %delay 2755359744, 11;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5f494c1ae210;
T_3 ;
    %vpi_call 2 37 "$monitor", "Time = %0t ns | clk_100hz = %b", $time, v0x5f494c1c1260_0 {0 0 0};
    %vpi_call 2 38 "$dumpfile", "clockdivider.vcd" {0 0 0};
    %vpi_call 2 39 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "clockdivider_tb.v";
    "clockdivider.v";
