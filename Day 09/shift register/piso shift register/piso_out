 .timescale 0 0;
v0x640a79296590_0 .var "clk", 0 0;
v0x640a79296650_0 .var "load", 0 0;
v0x640a79296720_0 .var "pin", 3 0;
v0x640a79296820_0 .var "rst", 0 0;
v0x640a792968f0_0 .net "sout", 0 0, v0x640a79296430_0;  1 drivers
S_0x640a792818e0 .scope module, "dut" "piso" 2 5, 3 1 0, S_0x640a79281750;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "load";
    .port_info 3 /INPUT 4 "pin";
    .port_info 4 /OUTPUT 1 "sout";
v0x640a79281ac0_0 .net "clk", 0 0, v0x640a79296590_0;  1 drivers
v0x640a79296090_0 .net "load", 0 0, v0x640a79296650_0;  1 drivers
v0x640a79296150_0 .net "pin", 3 0, v0x640a79296720_0;  1 drivers
v0x640a79296240_0 .net "rst", 0 0, v0x640a79296820_0;  1 drivers
v0x640a79296300_0 .var "shift_reg", 3 0;
v0x640a79296430_0 .var "sout", 0 0;
E_0x640a7927eef0 .event posedge, v0x640a79281ac0_0;
    .scope S_0x640a792818e0;
T_0 ;
    %wait E_0x640a7927eef0;
    %load/vec4 v0x640a79296240_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x640a79296300_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x640a79296430_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x640a79296090_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x640a79296150_0;
    %assign/vec4 v0x640a79296300_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x640a79296300_0;
    %parti/s 1, 3, 3;
    %assign/vec4 v0x640a79296430_0, 0;
    %load/vec4 v0x640a79296300_0;
    %parti/s 3, 0, 2;
    %concati/vec4 0, 0, 1;
    %assign/vec4 v0x640a79296300_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x640a79281750;
T_1 ;
    %vpi_call 2 7 "$monitor", " clk=%b | rst=%b | load=%b | pin=%b | sout=%b | time = %0t", v0x640a79296590_0, v0x640a79296820_0, v0x640a79296650_0, v0x640a79296720_0, v0x640a792968f0_0, $time {0 0 0};
    %vpi_call 2 8 "$dumpfile", "piso.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x640a79281750 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x640a79296590_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x640a79296820_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x640a79296650_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x640a79296720_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x640a79296820_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x640a79296650_0, 0, 1;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x640a79296720_0, 0, 4;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x640a79296650_0, 0, 1;
    %delay 80, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x640a79281750;
T_2 ;
    %delay 10, 0;
    %load/vec4 v0x640a79296590_0;
    %inv;
    %store/vec4 v0x640a79296590_0, 0, 1;
