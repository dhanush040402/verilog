#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x603c5bc724d0 .scope module, "prime_nbit_tb" "prime_nbit_tb" 2 1;
 .timescale 0 0;
P_0x603c5bc72660 .param/l "N" 0 2 2, +C4<00000000000000000000000000001000>;
v0x603c5bc869a0_0 .var/i "i", 31 0;
v0x603c5bc86a80_0 .var "num", 7 0;
v0x603c5bc86b70_0 .net "prime", 0 0, v0x603c5bc86870_0;  1 drivers
S_0x603c5bc72700 .scope module, "dut" "prime_nbit" 2 5, 3 1 0, S_0x603c5bc724d0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "num";
    .port_info 1 /OUTPUT 1 "prime";
P_0x603c5bc728e0 .param/l "N" 0 3 1, +C4<00000000000000000000000000001000>;
v0x603c5bc61130_0 .var/i "i", 31 0;
v0x603c5bc86790_0 .net "num", 7 0, v0x603c5bc86a80_0;  1 drivers
v0x603c5bc86870_0 .var "prime", 0 0;
E_0x603c5bc70050 .event anyedge, v0x603c5bc86790_0;
    .scope S_0x603c5bc72700;
T_0 ;
    %wait E_0x603c5bc70050;
    %load/vec4 v0x603c5bc86790_0;
    %pad/u 32;
    %cmpi/u 2, 0, 32;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x603c5bc86870_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x603c5bc86870_0, 0, 1;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x603c5bc61130_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x603c5bc61130_0;
    %load/vec4 v0x603c5bc61130_0;
    %mul;
    %load/vec4 v0x603c5bc86790_0;
    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x603c5bc86790_0;
    %pad/u 32;
    %load/vec4 v0x603c5bc61130_0;
    %mod;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x603c5bc86870_0, 0, 1;
T_0.4 ;
    %load/vec4 v0x603c5bc61130_0;
    %addi 1, 0, 32;
    %store/vec4 v0x603c5bc61130_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x603c5bc724d0;
T_1 ;
    %vpi_call 2 8 "$dumpfile", "prime_nbit.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x603c5bc724d0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x603c5bc869a0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x603c5bc869a0_0;
    %cmpi/s 256, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x603c5bc869a0_0;
    %pad/s 8;
    %store/vec4 v0x603c5bc86a80_0, 0, 8;
    %delay 10, 0;
    %load/vec4 v0x603c5bc869a0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x603c5bc869a0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x603c5bc724d0;
T_2 ;
    %vpi_call 2 19 "$monitor", "Time=%0t | num=%0d | prime=%b", $time, v0x603c5bc86a80_0, v0x603c5bc86b70_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
