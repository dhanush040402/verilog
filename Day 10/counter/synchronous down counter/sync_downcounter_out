:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x611e6b2c7d90 .scope module, "sync_downcounter_tb" "sync_downcounter_tb" 2 1;
 .timescale 0 0;
v0x611e6b2da6a0_0 .var "clk", 0 0;
v0x611e6b2da770_0 .net "q", 3 0, v0x611e6b2da470_0;  1 drivers
v0x611e6b2da840_0 .var "rst", 0 0;
S_0x611e6b2c7f20 .scope module, "dut" "sync_downcounter" 2 4, 3 1 0, S_0x611e6b2c7d90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "q";
v0x611e6b2c8150_0 .net "clk", 0 0, v0x611e6b2da6a0_0;  1 drivers
v0x611e6b2da470_0 .var "q", 3 0;
v0x611e6b2da550_0 .net "rst", 0 0, v0x611e6b2da840_0;  1 drivers
E_0x611e6b2c6c00 .event posedge, v0x611e6b2da550_0, v0x611e6b2c8150_0;
    .scope S_0x611e6b2c7f20;
T_0 ;
    %wait E_0x611e6b2c6c00;
    %load/vec4 v0x611e6b2da550_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v0x611e6b2da470_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x611e6b2da470_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x611e6b2da470_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x611e6b2c7d90;
T_1 ;
    %vpi_call 2 6 "$monitor", "clock = %d | reset = %d | count = %d | time = %0t |", v0x611e6b2da6a0_0, v0x611e6b2da840_0, v0x611e6b2da770_0, $time {0 0 0};
    %vpi_call 2 7 "$dumpfile", "sync_downcounter.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x611e6b2da6a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x611e6b2da840_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x611e6b2da840_0, 0, 1;
    %delay 160, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x611e6b2c7d90;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x611e6b2da6a0_0;
    %inv;
    %store/vec4 v0x611e6b2da6a0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
