#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x646d01707e40 .scope module, "perfect_tb" "perfect_tb" 2 1;
 .timescale 0 0;
v0x646d0171b720_0 .var "num", 15 0;
v0x646d0171b810_0 .net "perfect", 0 0, v0x646d0171b600_0;  1 drivers
S_0x646d01707fd0 .scope module, "dut" "perfect" 2 5, 3 1 0, S_0x646d01707e40;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "num";
    .port_info 1 /OUTPUT 1 "perfect";
v0x646d017081b0_0 .var "I", 15 0;
v0x646d0171b460_0 .var/i "i", 31 0;
v0x646d0171b540_0 .net "num", 15 0, v0x646d0171b720_0;  1 drivers
v0x646d0171b600_0 .var "perfect", 0 0;
E_0x646d01705380 .event anyedge, v0x646d0171b540_0, v0x646d017081b0_0;
    .scope S_0x646d01707fd0;
T_0 ;
    %wait E_0x646d01705380;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x646d017081b0_0, 0, 16;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x646d0171b460_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x646d0171b460_0;
    %load/vec4 v0x646d0171b540_0;
    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %div;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x646d0171b540_0;
    %pad/u 32;
    %load/vec4 v0x646d0171b460_0;
    %mod;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x646d017081b0_0;
    %pad/u 32;
    %load/vec4 v0x646d0171b460_0;
    %add;
    %pad/u 16;
    %store/vec4 v0x646d017081b0_0, 0, 16;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x646d017081b0_0;
    %addi 0, 0, 16;
    %store/vec4 v0x646d017081b0_0, 0, 16;
T_0.3 ;
    %load/vec4 v0x646d0171b460_0;
    %addi 1, 0, 32;
    %store/vec4 v0x646d0171b460_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x646d0171b540_0;
    %load/vec4 v0x646d017081b0_0;
    %cmp/e;
    %jmp/0xz  T_0.4, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x646d0171b600_0, 0, 1;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x646d0171b600_0, 0, 1;
T_0.5 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x646d01707e40;
T_1 ;
    %vpi_call 2 7 "$monitor", "number = %d | perfect = %b | time = %t", v0x646d0171b720_0, v0x646d0171b810_0, $time {0 0 0};
    %vpi_call 2 8 "$dumpfile", "perfect.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 6, 0, 16;
    %store/vec4 v0x646d0171b720_0, 0, 16;
    %delay 1, 0;
    %pushi/vec4 15, 0, 16;
    %store/vec4 v0x646d0171b720_0, 0, 16;
    %delay 1, 0;
    %pushi/vec4 28, 0, 16;
    %store/vec4 v0x646d0171b720_0, 0, 16;
    %delay 1, 0;
    %pushi/vec4 1000, 0, 16;
    %store/vec4 v0x646d0171b720_0, 0, 16;
    %delay 1, 0;
    %pushi/vec4 8128, 0, 16;
    %store/vec4 v0x646d0171b720_0, 0, 16;
    %delay 1, 0;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "perfect_tb.v";
    "perfect.v";
