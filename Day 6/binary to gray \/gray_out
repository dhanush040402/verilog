 .timescale 0 0;
    .port_info 0 /INPUT 4 "bin";
    .port_info 1 /OUTPUT 4 "gray";
L_0x5dec51a02b70 .functor XOR 4, v0x5dec51a02660_0, L_0x5dec51a029e0, C4<0000>, C4<0000>;
v0x5dec519efa80_0 .net *"_ivl_0", 3 0, L_0x5dec51a029e0;  1 drivers
v0x5dec51a02250_0 .net *"_ivl_2", 2 0, L_0x5dec51a028c0;  1 drivers
L_0x702ed8f57018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x5dec51a02330_0 .net *"_ivl_4", 0 0, L_0x702ed8f57018;  1 drivers
v0x5dec51a023f0_0 .net "bin", 3 0, v0x5dec51a02660_0;  1 drivers
v0x5dec51a024d0_0 .net "gray", 3 0, L_0x5dec51a02b70;  alias, 1 drivers
L_0x5dec51a028c0 .part v0x5dec51a02660_0, 1, 3;
L_0x5dec51a029e0 .concat [ 3 1 0 0], L_0x5dec51a028c0, L_0x702ed8f57018;
    .scope S_0x5dec519ef710;
T_0 ;
    %vpi_call 2 7 "$monitor", "time=%0t bin=%b gray=%b", $time, v0x5dec51a02660_0, v0x5dec51a02720_0 {0 0 0};
    %vpi_call 2 8 "$dumpfile", "binary_to_gray.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5dec51a027f0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5dec51a027f0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5dec51a027f0_0;
    %pad/s 4;
    %store/vec4 v0x5dec51a02660_0, 0, 4;
    %delay 1, 0;
    %load/vec4 v0x5dec51a027f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5dec51a027f0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
