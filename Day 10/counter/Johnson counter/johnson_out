:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x567beefd8670 .scope module, "johnson_tb" "johnson_tb" 2 1;
 .timescale 0 0;
v0x567beefec1a0_0 .var "clk", 0 0;
v0x567beefec270_0 .net "q", 3 0, v0x567beefebfa0_0;  1 drivers
v0x567beefec340_0 .var "rst", 0 0;
S_0x567beefd8800 .scope module, "dut" "johnson" 2 4, 3 1 0, S_0x567beefd8670;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "q";
v0x567beefc6ec0_0 .net "clk", 0 0, v0x567beefec1a0_0;  1 drivers
v0x567beefebfa0_0 .var "q", 3 0;
v0x567beefec080_0 .net "rst", 0 0, v0x567beefec340_0;  1 drivers
E_0x567beefd5c40 .event posedge, v0x567beefc6ec0_0;
    .scope S_0x567beefd8800;
T_0 ;
    %wait E_0x567beefd5c40;
    %load/vec4 v0x567beefec080_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x567beefebfa0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 1, 3, 3;
    %nor/r;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_0.4, 9;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 1, 0, 2;
    %nor/r;
    %and;
T_0.4;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 1, 3, 3;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_0.5, 9;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 1, 0, 2;
    %nor/r;
    %and;
T_0.5;
    %or;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 1, 0, 1;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x567beefebfa0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 1, 3, 3;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_0.8, 9;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 1, 0, 2;
    %and;
T_0.8;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 1, 3, 3;
    %nor/r;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_0.9, 9;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 1, 0, 2;
    %and;
T_0.9;
    %or;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %pushi/vec4 0, 0, 1;
    %load/vec4 v0x567beefebfa0_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x567beefebfa0_0, 0;
T_0.6 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x567beefd8670;
T_1 ;
    %vpi_call 2 6 "$monitor", "clock = %d | reset = %d | count = %b | time = %0t |", v0x567beefec1a0_0, v0x567beefec340_0, v0x567beefec270_0, $time {0 0 0};
    %vpi_call 2 7 "$dumpfile", "johnson.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x567beefec1a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x567beefec340_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x567beefec340_0, 0, 1;
    %delay 160, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x567beefd8670;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x567beefec1a0_0;
    %inv;
    %store/vec4 v0x567beefec1a0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
