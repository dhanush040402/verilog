    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x63f0bd630520_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x63f0bd630520_0;
    %pad/u 32;
    %cmpi/e 11, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x63f0bd630520_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x63f0bd630520_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x63f0bd630520_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x63f0bd61c9a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63f0bd630750_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x63f0bd630750_0;
    %inv;
    %store/vec4 v0x63f0bd630750_0, 0, 1;
    %jmp T_1.0;
    %end;
