#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 3;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x641441d07190 .scope module, "shiftled_tb" "shiftled_tb" 2 3;
 .timescale 0 -3;
v0x641441d2dd00_0 .var "clk", 0 0;
v0x641441d2ddc0_0 .net "led", 3 0, L_0x641441d2e060;  1 drivers
v0x641441d2de90_0 .var "rst", 0 0;
v0x641441d2df90_0 .var "shift_in", 0 0;
S_0x641441d196e0 .scope module, "dut" "shiftled" 2 6, 3 1 0, S_0x641441d07190;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "shift_in";
    .port_info 3 /OUTPUT 4 "led";
L_0x641441d2e060 .functor BUFZ 4, v0x641441d2db50_0, C4<0000>, C4<0000>, C4<0000>;
v0x641441d19930_0 .net "clk", 0 0, v0x641441d2dd00_0;  1 drivers
v0x641441d2d8e0_0 .net "led", 3 0, L_0x641441d2e060;  alias, 1 drivers
v0x641441d2d9c0_0 .net "rst", 0 0, v0x641441d2de90_0;  1 drivers
v0x641441d2da90_0 .net "shift_in", 0 0, v0x641441d2df90_0;  1 drivers
v0x641441d2db50_0 .var "shift_reg", 3 0;
E_0x641441d17ef0/0 .event negedge, v0x641441d19930_0;
E_0x641441d17ef0/1 .event posedge, v0x641441d19930_0;
E_0x641441d17ef0 .event/or E_0x641441d17ef0/0, E_0x641441d17ef0/1;
    .scope S_0x641441d196e0;
T_0 ;
    %wait E_0x641441d17ef0;
    %load/vec4 v0x641441d2d9c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x641441d2db50_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x641441d2db50_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x641441d2da90_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x641441d2db50_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x641441d07190;
T_1 ;
    %vpi_call 2 8 "$monitor", "reset = %d | shift input = %d | led out = %b | time = %0t |", v0x641441d2de90_0, v0x641441d2df90_0, v0x641441d2ddc0_0, $time {0 0 0};
    %vpi_call 2 9 "$dumpfile", "shiftled.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x641441d2dd00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x641441d2de90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x641441d2df90_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x641441d2de90_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x641441d2df90_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x641441d2df90_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x641441d2df90_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x641441d2df90_0, 0, 1;
    %delay 5000, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x641441d07190;
T_2 ;
    %delay 500, 0;
    %load/vec4 v0x641441d2dd00_0;
    %inv;
    %store/vec4 v0x641441d2dd00_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "shiftled_tb.v";
