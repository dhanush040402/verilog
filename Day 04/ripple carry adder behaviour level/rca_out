E_0x5ed6e1b1efd0 .event anyedge, v0x5ed6e1b34bc0_0, v0x5ed6e1b1e9b0_0, v0x5ed6e1b34ae0_0, v0x5ed6e1b34e50_0;
    .scope S_0x5ed6e1b1e7d0;
T_0 ;
    %wait E_0x5ed6e1b1efd0;
    %load/vec4 v0x5ed6e1b34bc0_0;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ed6e1b34e50_0, 4, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5ed6e1b34f30_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5ed6e1b34f30_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5ed6e1b1e9b0_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %load/vec4 v0x5ed6e1b34ae0_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %xor;
    %load/vec4 v0x5ed6e1b34e50_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %xor;
    %ix/getv/s 4, v0x5ed6e1b34f30_0;
    %store/vec4 v0x5ed6e1b34d20_0, 4, 1;
    %load/vec4 v0x5ed6e1b1e9b0_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %load/vec4 v0x5ed6e1b34ae0_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %and;
    %load/vec4 v0x5ed6e1b1e9b0_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %load/vec4 v0x5ed6e1b34e50_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %and;
    %or;
    %load/vec4 v0x5ed6e1b34ae0_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %load/vec4 v0x5ed6e1b34e50_0;
    %load/vec4 v0x5ed6e1b34f30_0;
    %part/s 1;
    %and;
    %or;
    %load/vec4 v0x5ed6e1b34f30_0;
    %addi 1, 0, 32;
    %ix/vec4/s 4;
    %store/vec4 v0x5ed6e1b34e50_0, 4, 1;
    %load/vec4 v0x5ed6e1b34f30_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5ed6e1b34f30_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x5ed6e1b34e50_0;
    %parti/s 1, 4, 4;
    %store/vec4 v0x5ed6e1b34c60_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5ed6e1b1e640;
T_1 ;
    %vpi_call 2 8 "$monitor", "Time = %t | A = %b | B = %b | Cin = %b | S = %b | C = %b ", $time, v0x5ed6e1b350b0_0, v0x5ed6e1b35190_0, v0x5ed6e1b35230_0, v0x5ed6e1b353d0_0, v0x5ed6e1b35300_0 {0 0 0};
    %vpi_call 2 9 "$dumpfile", "rca.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5ed6e1b350b0_0, 0, 4;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5ed6e1b35190_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ed6e1b35230_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5ed6e1b350b0_0, 0, 4;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x5ed6e1b35190_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ed6e1b35230_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x5ed6e1b350b0_0, 0, 4;
    %pushi/vec4 14, 0, 4;
    %store/vec4 v0x5ed6e1b35190_0, 0, 4;
