#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c69f297d570 .scope module, "led_blink_1hz_tb" "led_blink_1hz_tb" 2 1;
 .timescale 0 0;
v0x5c69f2991100_0 .var "clk", 0 0;
v0x5c69f29911d0_0 .net "led", 0 0, v0x5c69f2990ef0_0;  1 drivers
v0x5c69f29912a0_0 .var "rst", 0 0;
S_0x5c69f297d700 .scope module, "dut" "led_blink_1hz" 2 8, 3 5 0, S_0x5c69f297d570;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 1 "led";
P_0x5c69f297d8e0 .param/l "COUNT_MAX" 1 3 12, +C4<00000001011111010111100000111111>;
v0x5c69f296b640_0 .net "clk", 0 0, v0x5c69f2991100_0;  1 drivers
v0x5c69f2990e10_0 .var "count", 24 0;
v0x5c69f2990ef0_0 .var "led", 0 0;
v0x5c69f2990fc0_0 .net "rst", 0 0, v0x5c69f29912a0_0;  1 drivers
E_0x5c69f297b7d0 .event posedge, v0x5c69f296b640_0;
    .scope S_0x5c69f297d700;
T_0 ;
    %wait E_0x5c69f297b7d0;
    %load/vec4 v0x5c69f2990fc0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 25;
    %assign/vec4 v0x5c69f2990e10_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5c69f2990ef0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5c69f2990e10_0;
    %pad/u 32;
    %cmpi/e 24999999, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 25;
    %assign/vec4 v0x5c69f2990e10_0, 0;
    %load/vec4 v0x5c69f2990ef0_0;
    %inv;
    %assign/vec4 v0x5c69f2990ef0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5c69f2990e10_0;
    %addi 1, 0, 25;
    %assign/vec4 v0x5c69f2990e10_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5c69f297d570;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c69f2991100_0, 0, 1;
T_1.0 ;
    %delay 10, 0;
    %load/vec4 v0x5c69f2991100_0;
    %inv;
    %store/vec4 v0x5c69f2991100_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5c69f297d570;
T_2 ;
    %vpi_call 2 27 "$dumpfile", "ledblinking.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5c69f297d570 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c69f29912a0_0, 0, 1;
    %delay 40, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c69f29912a0_0, 0, 1;
    %delay 2000000000, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5c69f297d570;
T_3 ;
    %vpi_call 2 46 "$display", "Time (ns)\011Reset\011LED" {0 0 0};
    %vpi_call 2 47 "$monitor", "%0t\011%b\011%b", $time, v0x5c69f29912a0_0, v0x5c69f29911d0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "ledblinking_tb.v";
    "ledblinking.v";
