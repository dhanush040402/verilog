v0x5e44811cf820_0 .net "shift_l", 0 0, v0x5e44811cfed0_0;  1 drivers
v0x5e44811cf8e0_0 .net "shift_r", 0 0, v0x5e44811cffa0_0;  1 drivers
E_0x5e44811b8da0 .event posedge, v0x5e44811a7810_0;
    .scope S_0x5e44811b7f60;
T_0 ;
    %wait E_0x5e44811b8da0;
    %load/vec4 v0x5e44811cf630_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5e44811cf570_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5e44811cf6f0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %load/vec4 v0x5e44811cf570_0;
    %assign/vec4 v0x5e44811cf570_0, 0;
    %jmp T_0.7;
T_0.2 ;
    %load/vec4 v0x5e44811cf570_0;
    %assign/vec4 v0x5e44811cf570_0, 0;
    %jmp T_0.7;
T_0.3 ;
    %load/vec4 v0x5e44811cf8e0_0;
    %load/vec4 v0x5e44811cf570_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5e44811cf570_0, 0;
    %jmp T_0.7;
T_0.4 ;
    %load/vec4 v0x5e44811cf570_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x5e44811cf820_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5e44811cf570_0, 0;
    %jmp T_0.7;
T_0.5 ;
    %load/vec4 v0x5e44811cf490_0;
    %assign/vec4 v0x5e44811cf570_0, 0;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5e44811b7dd0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5e44811cfa80_0;
    %inv;
    %store/vec4 v0x5e44811cfa80_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5e44811b7dd0;
T_2 ;
    %vpi_call 2 15 "$monitor", "clock = %d | reset = %d | mode = %d | shift right = %b | shift left = %b | input = %b | output = %b | time = %0t ", v0x5e44811cfa80_0, v0x5e44811cfd10_0, v0x5e44811cfde0_0, v0x5e44811cffa0_0, v0x5e44811cfed0_0, v0x5e44811cfb40_0, v0x5e44811cfc10_0, $time {0 0 0};
    %vpi_call 2 16 "$dumpfile", "uni_shift.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e44811cfa80_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5e44811cfd10_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5e44811cfde0_0, 0, 2;
    %pushi/vec4 0, 0, 1;
