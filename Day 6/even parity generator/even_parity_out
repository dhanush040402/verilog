#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5752eaaa4390 .scope module, "even_parity_tb" "even_parity_tb" 2 1;
 .timescale 0 0;
v0x5752eaab5ff0_0 .var "data", 3 0;
v0x5752eaab60b0_0 .var/i "i", 31 0;
v0x5752eaab6170_0 .net "parity", 0 0, L_0x5752eaab6270;  1 drivers
S_0x5752eaaa4520 .scope module, "dut" "even_parity" 2 5, 3 1 0, S_0x5752eaaa4390;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "data";
    .port_info 1 /OUTPUT 1 "parity";
v0x5752eaaa4700_0 .net "data", 3 0, v0x5752eaab5ff0_0;  1 drivers
v0x5752eaab5ed0_0 .net "parity", 0 0, L_0x5752eaab6270;  alias, 1 drivers
L_0x5752eaab6270 .reduce/xor v0x5752eaab5ff0_0;
    .scope S_0x5752eaaa4390;
T_0 ;
    %vpi_call 2 7 "$monitor", "time=%0t data=%b parity=%b", $time, v0x5752eaab5ff0_0, v0x5752eaab6170_0 {0 0 0};
    %vpi_call 2 8 "$dumpfile", "even_parity.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x5752eaab60b0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5752eaab60b0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5752eaab60b0_0;
    %pad/s 4;
    %store/vec4 v0x5752eaab5ff0_0, 0, 4;
    %delay 1, 0;
    %load/vec4 v0x5752eaab60b0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5752eaab60b0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
