S_0x60b73588f3e0 .scope module, "comparator_4bit_tb" "comparator_4bit_tb" 2 1;
 .timescale 0 0;
v0x60b7358a5490_0 .var "A", 3 0;
v0x60b7358a5570_0 .net "A_equal", 0 0, L_0x60b7358a5a30;  1 drivers
v0x60b7358a5610_0 .net "A_greater", 0 0, L_0x60b7358a58a0;  1 drivers
v0x60b7358a56e0_0 .net "A_smaller", 0 0, L_0x60b7358a5b20;  1 drivers
v0x60b7358a57b0_0 .var "B", 3 0;
S_0x60b73588f570 .scope module, "dut" "comparator_4bit" 2 7, 3 1 0, S_0x60b73588f3e0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "A";
    .port_info 1 /INPUT 4 "B";
    .port_info 2 /OUTPUT 1 "A_greater";
    .port_info 3 /OUTPUT 1 "A_equal";
    .port_info 4 /OUTPUT 1 "A_smaller";
v0x60b73588f750_0 .net "A", 3 0, v0x60b7358a5490_0;  1 drivers
v0x60b7358a50a0_0 .net "A_equal", 0 0, L_0x60b7358a5a30;  alias, 1 drivers
v0x60b7358a5160_0 .net "A_greater", 0 0, L_0x60b7358a58a0;  alias, 1 drivers
v0x60b7358a5200_0 .net "A_smaller", 0 0, L_0x60b7358a5b20;  alias, 1 drivers
v0x60b7358a52c0_0 .net "B", 3 0, v0x60b7358a57b0_0;  1 drivers
L_0x60b7358a58a0 .cmp/gt 4, v0x60b7358a5490_0, v0x60b7358a57b0_0;
L_0x60b7358a5a30 .cmp/eq 4, v0x60b7358a5490_0, v0x60b7358a57b0_0;
L_0x60b7358a5b20 .cmp/gt 4, v0x60b7358a57b0_0, v0x60b7358a5490_0;
    .scope S_0x60b73588f3e0;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "comparator.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %vpi_call 2 12 "$display", "Time |   A   |   B   |  >  =  < " {0 0 0};
    %vpi_call 2 13 "$monitor", "%2t   | %b | %b |  %b  %b  %b", $time, v0x60b7358a5490_0, v0x60b7358a57b0_0, v0x60b7358a5610_0, v0x60b7358a5570_0, v0x60b7358a56e0_0 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x60b7358a5490_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x60b7358a57b0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x60b7358a5490_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x60b7358a57b0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x60b7358a5490_0, 0, 4;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x60b7358a57b0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x60b7358a5490_0, 0, 4;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x60b7358a57b0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x60b7358a5490_0, 0, 4;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x60b7358a57b0_0, 0, 4;
